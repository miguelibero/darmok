{
    "files": {
        "animator.json": "copy",
        "*.png": "copy",
        "Human@(*).fbx": {
            "skeletal_animation": {
                "skeleton": "BasicMotions_DummyModel.fbx",
                "useFileMatchAsName": 1,
                "outputPath": "*.ozz",
                "minKeyframeDuration": 0.03
            }
        },
        "BasicMotions_DummyModel.fbx": {
            "scene": {
                "outputPath": "scene.dsc",
                "program": "Forward",
                "skipNodes": [
                    "Rig",
                    "DummyType02",
                    "DummyType01_BodyOutline",
                    "DummyType01_HeadOutline"
                ]
            },
            "skeleton": {
                "outputPath": "skeleton.ozz"
            }
        }
    }
}