{
    "files": {
        "animator.json": "copy",
        "*.png": "copy",
        "Human@(*).fbx": {
            "skeletal_animation": {
                "skeleton": "BasicMotions_DummyModel.fbx",
                "useFileMatchAsName": 1,
                "outputPath": "*.ozz",
                "optimization": {
                    "tolerance" : 0.1,
                    "distance" : 0.1
                }
            }
        },
        "BasicMotions_DummyModel.fbx": {
            "model": {
                "outputPath": "model.dml",
                "program": "forward",
                "skipNodes": [
                    "Rig",
                    "DummyType02",
                    "DummyType01_BodyOutline",
                    "DummyType01_HeadOutline"
                ]
            },
            "skeleton": {
                "outputPath": "skeleton.ozz"
            }
        }
    }
}